{
  "name": "panic-frontend",
  "version": "0.0.1",
  "description": "Frontend for the Don't Panic Grocery App.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "dev": "set -a; source ../../environments/local_secret.env; webpack --mode development ./src/index.js --output ./static/frontend/main.js",
    "watch": "set -a; source ../../environments/local_secret.env; webpack --mode development ./src/index.js --watch --output ./static/frontend/main.js",
    "stage": "set -a; source ../../environments/stage_secret.env; webpack --mode production ./src/index.js --output ./static/frontend/main.js"
  },
  "keywords": [],
  "author": "niall@niallbyrne.ca",
  "license": "MPL 2.0",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "webpack": "^4.42.1",
    "webpack-bundle-tracker": "^0.4.3",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.1.2"
  }
}
