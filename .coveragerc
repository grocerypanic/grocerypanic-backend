[run]
branch = True
omit =
    */test*,
    */apps.py,
    */urls.py,
    */migrations/*.py,
    panic/config/environments/*.py,
    panic/manage.py,
    panic/root/asgi.py,
    panic/root/wsgi.py,
    panic/root/urls/*
source = panic

[report]
precision = 1
show_missing = True
ignore_errors = True
exclude_lines = nocover
